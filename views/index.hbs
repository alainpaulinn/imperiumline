<main class='c-app dark' id='c-app'>
  <div class='c-application' id="appWrapper">
    <aside class='c-sidepanel expanded' id='c-sidepanel'>
      {{> sidePanel}}
    </aside>
    <section id="messages_panel" class="c-messages_panel">
      {{> messagesPanel}}
    </section>
    <section class="c-call_log_panel" id="call_log_panel">
      {{> callsPanel}}
    </section>
    <section class="c-ongoing_call_panel" id="ongoing_call_panel">
      {{> ongoingCallPanel}}
    </section>
    <section class="c-time-scheduling_panel" id="time-scheduling_panel">
      {{> timeSchedulingPanel}}
    </section>
    <section class="c-all_users_panel" id="all_users_panel">
      {{> allUsersPanel}}
    </section>
  </div>
  <div class="notifications" id="notificationsDiv"></div>
  <div class="inscreenPanelContainer" id="inscreenPanelContainer"></div>
</main>


